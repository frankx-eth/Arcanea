{
  "name": "arcanea-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "web": "yarn workspace @arcanea/web dev",
    "app": "yarn workspace @arcanea/app start",
    "api": "yarn workspace @arcanea/api dev",
    "storybook": "yarn workspace @arcanea/storybook web",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.19"
}
